{
  "name": "@eyevinn/typescript-nodecli",
  "version": "1.0.0",
  "description": "Template CLI for TypeScript Node.js projects",
  "main": "dist/index.js",
  "bin": {
    "typescript-nodecli": "dist/cli.js"
  },
  "scripts": {
    "test": "jest",
    "prepare": "husky install",
    "lint": "eslint .",
    "pretty": "prettier --check --ignore-unknown .",
    "typecheck": "tsc --noEmit -p tsconfig.json",
    "start": "ts-node -T src/cli.ts"
  },
  "license": "MIT",
  "engines": {
    "node": ">=18.15.0"
  },
  "dependencies": {
    "commander": "^12.1.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.4.2",
    "@commitlint/config-conventional": "^17.4.2",
    "@types/jest": "^29.4.0",
    "@types/node": "^18.11.18",
    "@typescript-eslint/eslint-plugin": "^5.51.0",
    "@typescript-eslint/parser": "^5.51.0",
    "eslint": "^8.33.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "jest-environment-node-single-context": "^29.1.0",
    "prettier": "^2.8.4",
    "ts-jest": "^29.0.5",
    "typescript": "^4.9.5"
  }
}
